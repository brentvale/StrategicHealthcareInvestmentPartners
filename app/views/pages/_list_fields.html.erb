<fieldset>
  <%= f.label :list_name, "List Heading" %><br />
  <%= f.text_area :list_name, class: "heading-text" %><br />
  <%= f.check_box :_destroy %>
  <%= f.label :_destroy, "Remove List" %>
  
  <%= f.fields_for :list_items do |builder| %>
    <%= render 'list_item_fields', f: builder %>
  <% end %>
  <%= link_to_add_fields "Add List Item", f, :list_items %>
</fieldset>